{
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        {
            "command": "unbound",
            "keys": "ctrl+c"
        },
        {
            "command": 
            {
                "action": "copy",
                "singleLine": false
            }
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": "paste",
            "keys": "ctrl+shift+v"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        }
    ],
    "alwaysShowTabs": false,
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
    "disableAnimations": true,
    "experimental.rendering.forceFullRepaint": false,
    "experimental.rendering.software": false,
    "initialCols": 120,
    "initialRows": 30,
    "launchMode": "default",
    "profiles": 
    {
        "defaults": 
        {
            "bellStyle": "none",
            "colorScheme": "One Half Dark"
        },
        "list": 
        [
            {
                "acrylicOpacity": 0.22,
                "backgroundImage": null,
                "backgroundImageOpacity": 0.10000000000000001,
                "colorScheme": "Tango Dark",
                "commandline": "%userprofile%\\scoop\\apps\\pwsh\\current\\pwsh.exe -NoProfile -NoExit -Command %userprofile%\\p.ps1\"",
                "cursorShape": "filledBox",
                "experimental.retroTerminalEffect": false,
                "font": 
                {
                    "face": "FiraCode NF",
                    "size": 12
                },
                "guid": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
                "hidden": false,
                "intenseTextStyle": "all",
                "name": "PowerShell Advanced",
                "padding": "8",
                "scrollbarState": "hidden",
                "source": "Windows.Terminal.PowershellCore",
                "startingDirectory": "%USERPROFILE%",
                "suppressApplicationTitle": false
            },
            {
                "backgroundImage": null,
                "backgroundImageOpacity": 0.20000000000000001,
                "bellStyle": "none",
                "colorScheme": "Tango Dark",
                "commandline": "%userprofile%\\scoop\\apps\\neovim\\current\\bin\\nvim.exe -c \"VimwikiIndex\"\t",
                "cursorShape": "filledBox",
                "font": 
                {
                    "face": "FiraCode NF"
                },
                "guid": "{aa9f4ac8-0e4c-425f-9110-9f0cbc626a09}",
                "icon": "%userprofile%\\scoop\\apps\\neovim\\0.6.0\\share\\icons\\hicolor\\128x128\\apps\\nvim.png",
                "intenseTextStyle": "all",
                "name": "Wiki",
                "scrollbarState": "hidden",
                "startingDirectory": "%USERPROFILE%"
            }
        ]
    },
